        "react-dom": "^18.3.1",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.26.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    },
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.app",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.app",
                "preset": "iPhone 7"
            }
        }
