        "eslint-plugin-storybook": "^0.8.0",
        "file-loader": "^6.2.0",
        "globals": "^15.9.0",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^8.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "loki": "^0.35.1",
